	},
	"input-label":{
		"prefix":"!k-input-label",
		"description":"The K-scaffold input-label mixin",
		"body":[
			"+input-label({",
			"\tinputObj:{${1}}",
			"\tspanObj:{${2}}",
			"})${0}"
		]
	},
	"select-label":{
		"prefix":"!k-select-label",
		"description":"The K-scaffold select-label mixin",
		"body":[
			"+select-label({",
			"\tinputObj:{${1}}",
			"\tspanObj:{${2}}",
			"})",
			"\t${0}"
		]
	},
	"action-label":{
		"prefix":"!k-action-label",
		"description":"The K-scaffold action-label mixin",
		"body":[
			"+action-label({",
			"\tbuttonObj:{${1}}",
			"\tspanObj:{${2}}",
			"})${0}"
		]
	},
	"roller-label":{
		"prefix":"!k-roller-label",
		"description":"The K-scaffold roller-label mixin",
		"body":[
			"+roller-label({",
			"\tinputObj:{${1}}",
			"\tbuttonObj:{${2}}",
			"})${0}"
		]
	},
	"button-label":{
		"prefix":"!k-button-label",
		"description":"The K-scaffold button-label mixin",
		"body":[
			"+button-label({",
			"\tbuttonObj:{${1}}",
			"\tspanObj:{${2}}",
			"})${0}"
		]
	},
	"adaptiveTextarea":{
		"prefix":"!k-adaptiveTextarea",
		"description":"The K-scaffold adaptiveTextarea mixin",
		"body":[
			"+adaptiveTextarea(${1:textObj})${0}"
		]
	},
	"adaptiveInput":{
		"prefix":"!k-adaptiveInput",
		"description":"The K-scaffold adaptiveInput mixin",
		"body":[
			"+adaptiveInput(${1:textObj})${0}"
		]
	},
	"headedTextarea":{
		"prefix":"!k-headedTextarea",
		"description":"The K-scaffold headedTextarea mixin",
		"body":[
			"+headedTextarea({",
			"\theader:'${1}',",
			"\ttextObj:${2},",
			"})${0}"
		]
	},
	"compendiumAttributes":{
		"prefix":"!k-compendiumAttributes",
		"description":"The K-scaffold compendiumAttributes mixin",
		"body":[
			"+compendiumAttributes({prefix:'${1}'})${0}"
		]
	},
	"compendiumContainer":{
		"prefix":"!k-compendiumAttributes",
		"description":"The K-scaffold compendiumAttributes mixin",
		"body":[
			"+compendiumContainer({vttCategories:['${1}']})",
        "\t${0}"
		]
	},
	"Full Trigger":{
		"prefix":"!k-trigger",
		"description":"A trigger that includes all the likely trigger elements to be referenced in a pug declaration",
		"body":[
			"trigger:{",
			"\taffects:[${1:Array of attribute names that should be affected when this attribute is changed}],",
			"\tinitialFuncs:[${2:Array of function names to trigger only when this attribute is changed by the user}],",
			"\ttriggeredFuncs:[${3:Array of function names to trigger when this attribute is affected or button is clicked}]",
			"\tcalculation:${4:String name of the function to call to calculate the value of this attribute},",
			"}"
		]
	},
	"Simple affects trigger":{
		"prefix":"!k-affects",
		"description":"A simple trigger that just contains an affects property",
		"body":[
			"trigger:{affects:[${1:Array of attribute names that should be affected when this attribute is changed}]}"
		]
	},
	"Simple calculation trigger":{
		"prefix":"!k-calculation",
		"description":"A simple trigger that just contains a calculation property",
		"body":[
			"trigger:{calculation:${1:String name of the function to call to calculate the value of this attribute}}"
		]
	},
	"Simple formula trigger":{
		"prefix":"!k-formula",
		"description":"A simple trigger that just contains a calculation property",
		"body":[
			"trigger:{formula:${1:String Roll20 Macro expression}}"
		]
	},
	"Simple initialFuncs trigger":{
		"prefix":"!k-initialFuncs",
		"description":"A simple trigger that just contains the initialFuncs property",
		"body":[
			"trigger:{initialFuncs:[${1:Array of function names to trigger only when this attribute is changed by the user}]}"
		]
	},
	"Simple triggeredFuncs trigger":{
		"prefix":"!k-triggeredFuncs",
		"description":"A simple trigger that just contains a triggeredFuncs Property",
		"body":[
			"trigger:{triggeredFuncs:[${1:Array of function names to trigger when this attribute is affected or button is clicked}]}"
		]
	}
}