		]
	},
	"action":{
		"prefix":"!k-action",
		"description":"The generic K-scaffold action mixin",
		"body":[
			"+action({",
			"\tname:'${1:Button name}',${0}",
			"\ttrigger:{triggeredFuncs:['${2:Function Name to Call}']}",
			"})"
		]
	},
	"navButton":{
