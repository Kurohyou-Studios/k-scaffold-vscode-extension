{
  "input":{
		"prefix":"!k-input",
		"description":"The generic K-scaffold input mixin",
		"body":[
			"+input({name:'${1:Attribute name}',type:'${2|text,number,hidden,checkbox,radio,range|}',value:${0}})"
		]
	},
	"text":{
		"prefix":"!k-text",
		"description":"The generic K-scaffold text mixin",
		"body":[
			"+text({name:'${1:Attribute name}',value:'${0}'})"
		]
	},
	"number":{
